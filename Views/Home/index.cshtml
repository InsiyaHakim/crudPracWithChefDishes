@model User

<div class="container">
    <nav class="navbar navbar-expand-sm bg-light">
        <!-- Links -->
        <ul class="navbar-nav">
            <li class="nav-item">
            <a class="nav-link" href="/login_page">Login</a>
            </li>
        </ul>
    </nav>
    <div class="row">
        <div class="col-lg-12">
            <h1>Register</h1>
        </div>
    </div>
    <form asp-action="create" asp-controller="Home" method="post" class="needs-validation" novalidate>
        <div class="form-group">
            <label asp-for="f_name"></label>
            <input type="text" class="form-control" asp-for="f_name" id="">
            <span asp-validation-for="f_name"></span>
        </div>
        <div class="form-group">
            <label asp-for="l_name"></label>
            <input type="text" class="form-control" asp-for="l_name" id="">
            <span asp-validation-for="l_name"></span>
        </div>
        <div class="form-group">
            <label asp-for="email"></label>
            <input type="text" class="form-control" asp-for="email" id="">
            <span asp-validation-for="email"></span>
        </div>
        <div class="form-group">
            <label asp-for="password"></label>
            <input type="password" class="form-control" asp-for="password" id="">
            <span asp-validation-for="password"></span>
        </div>
        <div class="form-group">
            <label asp-for="c_password"></label>
            <input type="password" class="form-control" asp-for="c_password" id="">
            <span asp-validation-for="c_password"></span>
        </div>
        <center><input type="submit" class="btn btn-primary" value="Submit"></center> 
    </form>
</div>


